<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!-- Dark Theme Colors -->
	<Color x:Key="DarkMenu">#0A0A0A</Color>
	<Color x:Key="DarkSubMenu">#141414</Color>
	<Color x:Key="DarkContentArea">#1F1F1F</Color>
	<Color x:Key="DarkSep">#333333</Color>
	<Color x:Key="DarkMenuBtnGradientStart">#1F1F1F</Color>
	<Color x:Key="DarkMenuBtnGradientStop">#333333</Color>


	<!-- Light Theme Colors -->
	<Color x:Key="LightMenu">#EBEBEB</Color>
	<Color x:Key="LightSubMenu">#F0F0F0</Color>
	<Color x:Key="LightContentArea">#F5F5F5</Color>
	<Color x:Key="LightSep">#d9d9d9</Color>
	<Color x:Key="LightMenuBtn">#F5F5F5</Color>

</ResourceDictionary>
